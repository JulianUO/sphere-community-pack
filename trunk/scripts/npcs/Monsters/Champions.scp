               REF402=<ATTACKER.<dLOCAL.Y>>
               IF (<REF402.FLAGS>&STATF_PET)
                  REF402=<REF402.MEMORYFINDTYPE.memory_ipet.LINK>
               ENDIF
               SERV.B @66 <REF402.NAME> killed the Dark Father in Doom! Congratulations!
               REF402.NEWGOLD 5000
               IF (<R<REF402.LUCK>> > <R550>)
                  SERV.NEWITEM <HVAL <DEF.DOOMGAUNTLET_ARTIFACTS>>
                  REF402.BOUNCE <NEW>
                  REF402.sysmessage @,,2 1062317
               ENDIF
            ENDIF
         ENDIF
      ENDFOR
   ENDIF
   VAR0.DARKFATHER -= 1
   IF (<VAR0.DARKFATHER> <= 0)
