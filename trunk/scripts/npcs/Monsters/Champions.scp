//"The Harrower is the first incarnation of the Super Champion of the
// Felucca Dungeons. Not to be confused with the Tentacles of the Harrower
// from Khaldun, which is apparently only a small part of The True Harrower."
DEFNAME=c_harrower
NAME=harrower
ICON=i_pet_harrower
SOUND=snd_monster_shadowlord
CAN=MT_WALK|MT_RUN|MT_USEHANDS|MT_EQUIP
DAM=135,181
ARMOR=55
DESIRES=r_dungeon,t_coin,t_gold,t_gem,t_wand,t_potion,t_scroll,t_reagent
AVERSIONS=t_trap,r_civilization
//FOODTYPE=
//RESOURCES=
BLOODCOLOR=colors_green
MOVERATE=100
TAG.Barding.Diff=160.0
TAG.DamEnergy=50
TAG.IsChampion=1
TAG.IAMACHAMPION=1
//TAG.Luck.Loot=FIXME
CATEGORY=Monsters
SUBSECTION=Champions
DESCRIPTION=Harrower
ON=@Create
TAG.LIFETIER=2
   NPC=brain_monster
   FAME=25000
   KARMA=-25000
   STR={900 1000}
   MAXHITS={39000 41000}
   DEX={125 140}
   MAXSTAM={125 140}
   INT={1000 1200}
   MAXMANA={4900 5100}
   ARCHERY={90.0 100.0}
   EVALUATINGINTEL={120.0 125.0}
   FENCING={90.0 100.0}
   MACEFIGHTING={90.0 100.0}
   MAGERY={120.0 125.0}
   MAGICRESISTANCE={120.0 160.0}
   MEDITATION={120.0 125.0}
   PARRYING={90.0 100.0}
   SWORDSMANSHIP={90.0 100.0}
   TACTICS={90.0 110.0}
   WRESTLING={90.0 100.0}
   MODAR={0 10}
   RESCOLD={60 80}
   RESENERGY={60 80}
   RESFIRE={60 80}
   RESPOISON={60 80}
ON=@NPCRestock
   ITEM=loot_harrower
ON=@Death
   TAG0.LIFETIER -= 1
   IF (<TAG0.LIFETIER>)
      HITS=<MAXHITS>
      ACTION -1
      RETURN 1
   ENDIF
ON=@NPCLookAtChar
   //Teleport players closer
   IF (<SRC.ISPLAYER>) && !(<SRC.ISGM>)
      IF !(<R20>)
         SRC.GO <P>
         SAY @,,2 1049503//"Come get some..."
      ENDIF
   ENDIF
   //Explode corpses
   IF (<ISNEARTYPE T_CORPSE 10>)
      IF (<R20> == 1)
      REF1=<UID>
         FORITEMS 10
            IF (<BASEID> == i_corpse) && (!(<LOCAL.JAEXPLODIU>))
            F_EXPLODECORPSE <REF1.UID>
            F_EXPLODECORPSE_DROPALL
            LOCAL.JAEXPLODIU=1
            ENDIF
         ENDFOR
      ENDIF
   ENDIF
ON=@DeathCorpse
   LOCAL.NEW=<NEW>
   SERV.NEWNPC c_true_harrower
   NEW.GO <P>
   NEW.SAY @,,2 1049499//"Behold my true form!"
   REF1=<NEW>
   FOR 12
      SERV.NEWNPC c_harrower_tentacles
      NEW.GO <P>
      NEW.FACE <REF1.UID>
      NEW.MOVE <HVAL {-6 6}> <HVAL {-6 6}>
   ENDFOR
   NEW=<LOCAL.NEW>
   ARGO.REMOVE
ON=@Hit
   IF (!(<R20>))
      DORAND 4
      SAY @color_text,,2 1049500//"Feel the delicious pain!"
      SAY @color_text,,2 1049501//"You fight like a mongbat!"
      SAY @color_text,,2 1049505//"Your life will be mine!"
      SAY @color_text,,2 1049507//"Now you shall die."
      ENDDO
   ENDIF
ON=@SpellEffect
   SAY @color_text,,2 1049502//"You call that a spell?"
ON=@GetHit
   IF (!(<R20>))
      DORAND 2
      SAY @,,2 1049508//"You will never steal my power!"
      SAY @,,2 1049504//"Oooh, that tickles."
      ENDDO
   ENDIF
   IF !(<R30>)
      SRC.GO <P>
      SAY @,,2 1049503//"Come get some..."
   ENDIF
   IF (<SRC.FLAGS>&statf_conjured)
      IF (!(<R50>))
         SRC.SPELLEFFECT S_DISPEL,1000,<UID>
      ENDIF
   ENDIF

